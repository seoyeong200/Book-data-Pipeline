    "HandlerLambdaFunctionQualifiedArn": {
      "Description": "Current Lambda function version",
      "Value": {
        "Ref": "HandlerLambdaVersionFNN6zVG86QVqWPQGu5Tz1M3mzKufRY8XtJqgFKANQA"
      },
      "Export": {
