    "HandlerLambdaVersionFNN6zVG86QVqWPQGu5Tz1M3mzKufRY8XtJqgFKANQA": {
      "Type": "AWS::Lambda::Version",
      "DeletionPolicy": "Retain",
      "Properties": {
        "FunctionName": {
          "Ref": "HandlerLambdaFunction"
        },
        "CodeSha256": "000b57e76ee802ba1cc3ebad9a7f004170541b8a5aa4cc5953ad1c935db7fff7"
      }
    }
  },
  "Outputs": {
